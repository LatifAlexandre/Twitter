<?php

/*
 id  | identifiant (login) | pass|  nom   | prenom | statut |   avatar  
   
*/


class Utilisateur extends basemodel
{
	/*
	par conventions les variables d'un utilisateur sont nommé ainsi :
		id
		login (identifiant dans la bdd)
		pass
		firstName (prenom dans la bdd)
		lastName (nom dans la bdd)
		statut
		avatar
	*/
		
}
